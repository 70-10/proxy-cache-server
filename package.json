{
  "name": "proxy-cache-server",
  "version": "0.1.0",
  "description": "A caching proxy server for API responses, perfect for development and testing environments",
  "keywords": ["proxy", "cache", "server", "api", "development", "testing", "cli"],
  "license": "MIT",
  "homepage": "https://github.com/70-10/proxy-cache-server",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/70-10/proxy-cache-server.git"
  },
  "author": "70-10",
  "type": "module",
  "bin": {
    "proxy-cache-server": "dist/bin/index.js",
    "pcs": "dist/bin/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "bun run bin/index.ts",
    "fix": "biome check --write ./src",
    "compile": "bun build ./bin/index.ts --target bun --outdir ./dist/bin --minify && chmod +x ./dist/bin/index.js",
    "compile:node": "bun build ./bin/index.ts --target node --outdir ./dist/bin --minify && chmod +x ./dist/bin/index.js",
    "build": "bun run compile:node",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "citty": "0.1.6",
    "hono": "4.6.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "latest"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
